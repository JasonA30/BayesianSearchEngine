                        result = Match.Family;
                        Tokenizer.nextToken();
                    }
                    else if (token.tokenID == Token.Plus) {
                        result = Match.Above;
                        Tokenizer.nextToken();
                    }
                }
                else if (token.tokenID == Token.Star) {
                    result = Match.Family;
                    Tokenizer.nextToken();
                }
                else if (token.tokenID == Token.Plus) {
                    result = Match.Above;
                    Tokenizer.nextToken();
                }

