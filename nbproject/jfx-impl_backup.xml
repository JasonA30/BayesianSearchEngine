                FCurrentToken = {
                    token: token,
                    tokenID: tokenID
                }

                return FCurrentToken;
            }

            return {
                start: start,
                nextToken: nextToken,
                pushToken: pushToken,
                currentToken: currentToken,
                isDigit: isDigit,
                isLetter: isLetter
