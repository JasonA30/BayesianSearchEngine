                for (index = 0 ;index < query.version.length && index < version.version.length;
                     index++) {
                    var q = query.version[index];
                    var v = version.version[index];

                    if (parseInt(q) == parseInt(v)) {
                        result = true;
                    }
                    else {
                        result = false;
                        break;
                    }
                }
